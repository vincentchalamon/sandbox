{
    "name": "vince/sandbox",
    "type": "project",
    "license": "MIT",
    "description": "The \"Vince Cms\" sandbox",
    "version": "1.0-dev",
    "keywords": ["CMS", "Vince"],
    "homepage": "http://vincentchalamon.github.io/sandbox/",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "authors": [
        {
            "name": "Vincent CHALAMON",
            "email": "vincentchalamon@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "elasticsearch/binaries",
                "version": "1.0.1",
                "dist": {
                    "url": "https://download.elasticsearch.org/elasticsearch/elasticsearch/elasticsearch-1.0.1.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/elasticsearch/elasticsearch.git",
                    "type": "git",
                    "reference": "v1.0.1"
                },
                "require": {
                    "friendsofsymfony/elastica-bundle": "2.1.*"
                }
            }
        }
    ],
    "require-dev": {
        "liip/functional-test-bundle": "1.0.0",
        "sensio/generator-bundle": "~2.3"
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "~2.5",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",

        "sensio/distribution-bundle": "~3.0",
        "sensio/framework-extra-bundle": "~3.0",

        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "doctrine/migrations": "1.0.*@dev",
        "doctrine/doctrine-migrations-bundle": "2.1.*@dev",

        "twig/extensions": "~1.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "nervo/yuicompressor": "2.4.8",

        "friendsofsymfony/user-bundle": "~1.3",
        "sonata-project/core-bundle": "~2.2",
        "sonata-project/admin-bundle": "~2.2",
        "sonata-project/user-bundle": "~2.2",
        "sonata-project/doctrine-orm-admin-bundle": "~2.2",
        "vince/cms-sonata-admin-bundle": "~1"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-assets-install": "symlink",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}