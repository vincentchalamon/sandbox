{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}
    {% spaceless %}
        {%- if value is empty -%}
            &nbsp;
        {%- else -%}
            {% autoescape false %}{{ value|raw }}{% endautoescape %}
        {%- endif -%}
    {% endspaceless %}
{% endblock %}