{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}
    {% spaceless %}
        {%- if value is empty -%}
            &nbsp;
        {%- else -%}
            <a href="{{ value }}" target="_blank" class="url">{{ value }}</a>
        {%- endif -%}
    {% endspaceless %}
{% endblock %}